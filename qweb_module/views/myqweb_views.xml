<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="myqweb_view_action" model="ir.actions.act_window">
        <field name="name">My Qweb</field>
        <field name="res_model">myqweb.form</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="myqweb_view_form" model="ir.ui.view">
        <field name="name">My Qweb Form</field>
        <field name="model">myqweb.form</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="surnames"/>
                        <field name="address"/>
                        <field name="phone"/>
                        <field name="email"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>